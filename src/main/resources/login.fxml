      </Label>
      <Label layoutX="296.0" layoutY="73.0" text="Email">
         <font>
            <Font name="Calibri Bold Italic" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="nameid" layoutX="3.0" layoutY="99.0" prefHeight="25.0" prefWidth="104.0" />
      <TextField fx:id="surnameid" layoutX="139.0" layoutY="99.0" prefHeight="25.0" prefWidth="130.0" />
      <TextField fx:id="emailid" layoutX="296.0" layoutY="99.0" />
      <Label fx:id="errorlable" layoutX="139.0" layoutY="138.0" prefHeight="36.0" prefWidth="158.0" text="Couldn't find the user" textAlignment="CENTER" textFill="RED" visible="false">
         <font>
            <Font name="Calibri" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
