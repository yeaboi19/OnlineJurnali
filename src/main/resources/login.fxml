<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="login" prefHeight="237.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFx.LoginFxmlController">
   <children>
      <Button layoutX="14.0" layoutY="197.0" mnemonicParsing="false" onAction="#onLoginPressed" text="Login">
